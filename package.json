  "scripts": {
    "start": "./node_modules/.bin/gulp build",
    "lint": "./node_modules/.bin/gulp lint",
    "prepare": "node scripts/copy-hooks.js && ./node_modules/.bin/jspm install && ./node_modules/.bin/typings install",
    "test": "./node_modules/.bin/gulp test"
  },
  "private": true
