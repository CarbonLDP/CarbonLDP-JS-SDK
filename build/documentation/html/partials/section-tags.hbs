<div class="ui list doc-tags">
	<div class="item">
		<div class="content">
			<span class="header">{{type}}:</span>
		</div>
		<div class="ui bulleted list">
			{{#each tags}}
				<div class="item">
					{{>text-markdown element=(default name this) noParagraph=true}}
				</div>
			{{/each}}
			{{#with tag}}
				<div class="item">
					{{>text-markdown element=(default name this) noParagraph=true}}
				</div>
			{{/with}}
		</div>
	</div>
</div>