# CarbonLDP SDK API Reference

{{>toc }}

# CarbonLDP SDK API

{{#each modules}}

	{{#with path~}}
		## <a name="Module-{{sanitize this}}"/>Module {{this}}
	{{/with}}

	{{#with description~}}
		> {{{trim this}}}
	{{/with}}

	{{#with default-export~}}
		**Default export:** [{{name}}](#{{sanitize name}})
		{{#with description~}}
			> {{{trim this}}}
		{{/with}}
	{{/with}}

	{{~> reexports}}

	{{#if enums~}}
		### <a name="{{sanitize path}}-Enums"/>Enums
		{{#each enums}}
			{{~>enum this}}
		{{/each}}
	{{/if}}

	{{#if properties~}}
		### <a name="{{sanitize path}}-Properties"/>Properties

		{{~> properties properties=properties.static access="static " }}

		{{~> properties properties=properties.instance access="" }}
	{{/if}}

	{{#if methods~}}
		### <a name="{{sanitize path}}-Methods"/>Methods

		{{~>methods methods=methods.static access="static " }}

		{{~>methods methods=methods.instance access="" }}
	{{/if}}
	{{#each interfaces}}

		{{#with path~}}
			### <a name="{{sanitize this}}"/>Interface {{this}}
		{{/with}}

		{{#if super-classes~}}
			**Extends:** {{#each super-classes}}[{{name}}](#{{sanitize name}}){{^if @last}}, {{/if}}{{/each}}
		{{/if}}

		{{#with description~}}
			> {{{trim this}}}
		{{/with}}

		{{#if properties~}}
			#### <a name="{{sanitize path}}-Properties"/>Properties

			{{~>properties properties=properties access="" }}
		{{/if}}

		{{#if methods~}}
			#### <a name="{{sanitize path}}-Methods"/>Methods

			{{~>methods methods=methods access="" }}
		{{/if}}
	{{/each}}
	{{#each classes}}

		{{#with path~}}
			### <a name="{{sanitize this}}"/>Class {{this}}
		{{/with}}

		{{#if super-classes~}}
			**Extends:** {{#each super-classes}}[{{name}}](#{{sanitize name}}){{^if @last}}, {{/if}}{{/each}}
		{{/if}}

		{{#with description~}}
			> {{{trim this}}}
		{{/with}}

		{{~> reexports}}

		{{#with constructors~}}
			#### <a name="{{sanitize ../path}}-Constructor"/>Constructor

			{{~>signatures name=../name }}
		{{/with}}

		{{#if properties~}}
			#### <a name="{{sanitize path}}-Properties"/>Properties

			{{~>properties properties=properties.static access="static " }}

			{{~>properties properties=properties.instance access="" }}
		{{/if}}

		{{#if methods~}}
			#### <a name="{{sanitize path}}-Methods"/>Methods

			{{~>methods methods=methods.static access="static " }}

			{{~>methods methods=methods.instance access="" }}
		{{/if}}

		{{#with decorated-object~}}
			#### <a name="{{sanitize ../path}}-Decorated-Object"/>Decorated Object
			{{#if type~}}
				**Interfaces:** {{#each type}}[{{this}}](#{{sanitize this}}){{^if @last}}, {{/if}}{{/each}}
			{{/if}}

			{{#with description~}}
				> {{{trim this}}}
			{{/with}}

			{{#if properties ~}}
				##### <a name="{{sanitize ../path}}-Decorated-Object-Properties"/>Properties

				{{~>properties properties=properties.instance access=""}}
			{{/if}}

			{{#if methods~}}
				##### <a name="{{sanitize ../path}}-Decorated-Object-Methods"/>Methods

				{{~>methods methods=methods.instance access=""}}
			{{/if}}
		{{/with}}
	{{/each}}
{{/each}}